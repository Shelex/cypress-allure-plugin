{
    "name": "@shelex/cypress-allure-plugin",
    "version": "0.0.0-development",
    "description": "allure reporting plugin for cypress",
    "main": "reporter/index.js",
    "types": "reporter/index.d.ts",
    "license": "Apache-2.0",
    "author": {
        "name": "Oleksandr Shevtsov",
        "email": "ovr.shevtsov@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Shelex/cypress-allure-plugin.git"
    },
    "files": [
        "reporter",
        "writer.js"
    ],
    "bugs": "https://github.com/Shelex/cypress-allure-plugin/issues",
    "publishConfig": {
        "registry": "http://registry.npmjs.org/"
    },
    "keywords": [
        "cypress",
        "allure",
        "allure2",
        "reporter"
    ],
    "scripts": {
        "semantic-release": "semantic-release",
        "test": "yarn cypress run --config integrationFolder=cypress/integration/results,testFiles=**/*.js,video=false",
        "test:prepare": "node cypress/scripts/runner cucumber && node cypress/scripts/runner basic",
        "fixtures:clear": "rm -r cypress/fixtures/*"
    },
    "dependencies": {
        "allure-js-commons": "^2.0.0-beta.6"
    },
    "devDependencies": {
        "@types/fs-extra": "^8.1.0",
        "@types/mocha": "^7.0.2",
        "cypress": "^4.5.0",
        "cypress-cucumber-preprocessor": "^2.3.1",
        "semantic-release": "^17.0.4"
    },
    "cypress-cucumber-preprocessor": {
        "nonGlobalStepDefinitions": true
    }
}
